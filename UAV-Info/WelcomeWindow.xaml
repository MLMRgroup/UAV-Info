<Window x:Class="UAV_Info.WelcomeWindow"        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        xmlns:local="clr-namespace:UAV_Info"        mc:Ignorable="d"        WindowStyle="None"          AllowsTransparency="True"        Title="WelcomeWindow" Height="238" Width="600"        WindowStartupLocation="CenterScreen">    <Grid >        <DockPanel>            <Canvas Name="canvasWelcome" Height="230" Width="600"  DockPanel.Dock="Top" >                <Image Name="myImage" Source="bg.png" Height="240"  Width="600" ></Image>            </Canvas>            <ProgressBar DockPanel.Dock="Bottom" Height="8" x:Name="ProgressBar" Minimum="0" Maximum="1" IsIndeterminate="False" />        </DockPanel>    </Grid></Window>